{
  "name": "peaky-server",
  "version": "1.0.0",
  "main": "src/server.ts",
  "repository": "git@github.com:aronreisx/peaky-barber.git",
  "author": "aronreisx <aronreis2@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "pm2 start --no-autorestart dist/server.js",
    "dev:ts": "tsnd --inspect=14007 --transpile-only --ignore-watch node_modules -r dotenv/config src/server.ts",
    "dev:js": "nodemon dist/server.js",
    "build": "tsc",
    "test": "jest",
    "typeorm": "tsnd ./node_modules/typeorm/cli.js",
    "m:run": "yarn typeorm migration:run",
    "m:create": "yarn typeorm migration:create -n",
    "m:show": "yarn typeorm migration:show",
    "m:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "argon2": "^0.28.3",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  }
}
